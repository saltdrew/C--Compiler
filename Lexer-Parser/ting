
union VALUE *walk(NODE *term){
    switch(term->type){
    	union VALUE result;
    	=malloc(sizeof(int));
    	case LEAF:
    		result.integer = ((TOKEN *)term)->value;
        	return &result;
    	case RETURN:
			return walk(term->left);
		default:
			printf("defaulted in walk");
			result.integer=0;
			return &result;
    }




}



    union VALUE *result= walk(tree);
    printf("the answer is %d\n", *result);
